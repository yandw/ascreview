{
  "manifest_version": 3,
  "name": "ASC Review",
  "version": "1.0.0",
  "description": "一个支持侧边栏启动的浏览器插件",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "sidePanel"
  ],
  "host_permissions": [
    "https://msasg.visualstudio.com/*"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}